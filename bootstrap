#! /bin/sh
echo "Update submodules"
git submodule update --init
cd platform/linux-dpaa2/flib/qbman/.
git am ../workaround_patches/0001-Patch-to-enable-batch-buffers-send-in-TX-path.patch
cd -

set -x
aclocal -I config -I m4
libtoolize --copy
autoheader
automake --add-missing --copy --warnings=all
autoconf --force
